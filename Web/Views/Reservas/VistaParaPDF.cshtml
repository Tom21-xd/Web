
@{
    Layout = null;
}

<!DOCTYPE html>

<html lang="en" data-bs-theme="dark">
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Reporte Reservas</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-T3c6CoIi6uLrA9TneNEoa7RxnatzjcDSCmG1MXxSR1GAsXEV/Dwwykc2MPK8M2HN" crossorigin="anonymous">

</head>
<body class="text-center">
    <h1 class="mt-5">Lista de Reservas</h1>
    <div class="m-5">
        <table class="table table-dark table-striped border-success text-center align-middle">
            <caption>Lista de Reservas</caption>
            <thead>
                <tr>
                    <th>Fecha Reserva</th>
                    <th>Fecha Creacion</th>
                    <th>Usuario</th>
                    <th>Servicio</th>
                    <th>Empleado</th>
                    <th>Estado</th>
                    <th>Espacio</th>

                </tr>
            </thead>
            <tbody>
                @foreach (ReservaModel re in ViewBag.reserva1)
                {
                    <tr>
                        <td>@re.FechaReserva</td>
                        <td>@re.FechaCreacion</td>
                        <td>@re.Usuario</td>
                        <td>@re.Servicio</td>
                        <td>@re.Empleado</td>
                        <td>@re.Estado</td>
                        <td>@re.Espacio</td>
                    </tr>

                }
            </tbody>
        </table>
    </div>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/js/bootstrap.bundle.min.js" integrity="sha384-C6RzsynM9kWDrMNeT87bh95OGNyZPhcTNXj1NW7RuBCsyN/o0jlpcV8Qyq46cDfL" crossorigin="anonymous"></script>

</body>

</html>